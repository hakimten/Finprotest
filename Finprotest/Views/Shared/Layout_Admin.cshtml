    <script>
        function checkInternetConnection() {
            var online = navigator.onLine;
            var modal = new bootstrap.Modal(document.getElementById("noInternetModal"));

            if (online) {
                modal.hide();
            } else {
                modal.show();
            }
        }

        setInterval(checkInternetConnection, 5000);
    </script>
    <script>
        // Redirect to selected link when an item is selected from the datalist
        var searchInput = document.querySelector('input[list="search-list"]');
        searchInput.addEventListener('input', function () {
            var selectedItem = document.querySelector('option[value="' + searchInput.value + '"]');
            if (selectedItem) {
                var link = selectedItem.dataset.link;
                window.location.href = link;
            }
        });

        // Sort options in datalist by first alphabet
        var datalist = document.getElementById('search-list');
        var options = Array.prototype.slice.call(datalist.options);
        options.sort(function (a, b) {
            if (a.value < b.value) { return -1; }
            else if (a.value > b.value) { return 1; }
            else { return 0; }
        });
        datalist.innerHTML = '';
        options.slice(0, 5).forEach(function (option) {
            datalist.appendChild(option);
        });

        // Update the datalist options based on the user input
        searchInput.addEventListener('input', function () {
            var inputValue = searchInput.value.toLowerCase();
            var filteredOptions = options.filter(function (option) {
                return option.value.toLowerCase().startsWith(inputValue);
            });
            datalist.innerHTML = '';
            filteredOptions.slice(0, 5).forEach(function (option) {
                datalist.appendChild(option);
            });
        });

        // Show only the first 5 options in the datalist when the input is clicked
        searchInput.addEventListener('click', function (e) {
            e.stopPropagation(); // Stop the event from propagating to the document
            if (datalist.children.length === 0) { // Only show options if the datalist is empty
                options.slice(0, 5).forEach(function (option) {
                    datalist.appendChild(option.cloneNode(true)); // Clone the option node and append to datalist
                });
            }
        });
    </script>
